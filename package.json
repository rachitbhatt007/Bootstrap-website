{
  "name": "bootstrap_cheatsheet",
  "version": "1.0.0",
  "description": "\"This is a test directory to learn git and node\"",
  "main": "aboutus.html",
  "scripts": {
    "start": "npm run lite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
